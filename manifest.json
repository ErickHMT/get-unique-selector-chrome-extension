{
    "manifest_version": 3,
    "name": "Unique selector",
    "description": "",
    "version": "1.0",
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://*/*", "http://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "css": ["content.css"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
        "scripting", "activeTab"
    ]
  }